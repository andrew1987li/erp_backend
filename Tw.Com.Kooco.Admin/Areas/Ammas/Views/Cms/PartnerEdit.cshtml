@using Newtonsoft.Json
@using Tw.Com.Kooco.Admin.Areas.Ammas.Entitys
@model Tw.Com.Kooco.Admin.Models.InternalDataTransferToView

@{
    var data = (Partner)Model.Data;
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
    var js = $"<script type='text/javascript'>var Partner = {JsonConvert.SerializeObject(Model.Data)};</script>";
}

@section FooterScript{@MvcHtmlString.Create(js)}

<div class="notifications bottom-left"></div>

<div class="portlet-body form">
    <!-- BEGIN FORM-->


    <div class="form-body">
        <div class="form-group required">
            <label class="col-md-3 control-label">Left Img</label>
            <div class="col-md-9">
                <span class="btn btn-success fileinput-button" style="padding: 4px 12px;">
                    <i class="glyphicon glyphicon-plus"></i>
                    <span>Add files...</span>
                    <input id="imgFileupload" type="file" name="file" multiple>
                </span>
                <br />
                <div class="progress progress-striped active" style="width: 80%; margin-bottom: 3px;">
                    <div id="progress-bar-left" class="progress-bar" role="progressbar" aria-valuenow="45" aria-valuemin="0" aria-valuemax="100" style="width: 0;">
                        <span class="sr-only"></span>
                    </div>
                </div>
                <img id="img" src="@data.ImgPath" alt="" />
            </div>
        </div>

       

    </div>
    <!-- END FORM-->
</div>